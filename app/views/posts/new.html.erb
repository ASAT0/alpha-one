<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-6 offset-md-3">
                    <%= form_for(@post) do |f| %>
                        <div class="col-md-10 offset-md-1">
                            <h4 class="text-center">タイトル</h4>
                            <%= f.text_field :name %>
                            <h4 class="text-center">本文</h4>
                            <%= f.text_area :body %>
                            <h4 class="text-center">写真</h4>
                            <%= f.file_field :images, multiple: true %>
                            <%= f.submit '投稿' %>
                        </div>
                    <% end %>
                </div>
                <div class="col-md-6 offsst-md-3">

                </div>
            </div>
        </div>
    </div>
</div>



